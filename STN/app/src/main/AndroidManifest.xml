<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.miwfem.socialtourismnetwork">

    <application
        android:name="com.miwfem.socialtourismnetwork.application.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_compass_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_compass_app_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SocialTourismNetwork">
        <activity
            android:name="com.miwfem.socialtourismnetwork.presentation.ui.main.MainActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>